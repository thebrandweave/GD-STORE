{
    "name": "goldendream/shop",
    "description": "GoldenDream Shop - E-commerce platform",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "firebase/php-jwt": "^6.0",
        "phpoffice/phpspreadsheet": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
